{
    "aliosStudio.inner.yosBin": "aos",
    "aliosStudio.hardware.board": "esp32devkitc",
    "aliosStudio.name": "hellotrustx",
    "aliosStudio.isAosSource": true,
    "aliosStudio.sdkPath": "${workspaceRoot}",
    "aliosStudio.comPort": "COM72",
    "aliosStudio.hardware.baudrate": 115200,
    "files.associations": {
        "pal.h": "c",
        "ifx_i2c.h": "c",
        "aos.h": "c",
        "pal_gpio.h": "c",
        "i2c.h": "c",
        "periph_ctrl.h": "c",
        "pal_os_event.h": "c",
        "k_api.h": "c",
        "timer.h": "c",
        "hal.h": "c",
        "vfs_device.h": "c",
        "vfs_gpio.h": "c",
        "vfs_register.h": "c",
        "soc.h": "c",
        "gpio.h": "c"
    }
    "multiCommand.commands": [
        {
            "command": "multiCommand.killall",
            "label": "kill all terminals",
            "sequence": [
                "workbench.action.terminal.kill",
                "workbench.action.terminal.kill"                                       
            ]
        },
        {
            "command": "multiCommand.build",
            "label": "kill terminals and build",
            "interval": 1,
            "sequence": [
                "multiCommand.killall",
                "alios-studio.build"                            
            ]
        },
        {
            "command": "multiCommand.upload",
            "label": "kill terminals and upload",
            "interval": 20000,
            "sequence": [
                "multiCommand.killall",
                "alios-studio.upload",
                "alios-studio.connectDevice"                          
            ]
        },
        {
            "command": "multiCommand.run",
            "label": "kill terminals and run the program",
            "interval": 1,
            "sequence": [
                "multiCommand.killall",               
                "alios-studio.connectDevice"
            ]
        }
    ]
}
 